/{
	aliases {
		spi2 = &spi_2;
	};
};

&soc {
	tlmm_pinmux: pinctrl@1000000 {
		spi2_active {
			qcom,pins = <&gp 8>, <&gp 9>,<&gp 10>, <&gp 11>;
			qcom,num-grp-pins = <4>;
			qcom,pin-func = <1>;
			label = "spi2_active";
			spi_2_active: spi_2_active {
				drive-strength = <12>;
				bias-disable = <0>;
			};
		};
		spi2_sleep {
			qcom,pins = <&gp 8>, <&gp 9>,<&gp 10>, <&gp 11>;
			qcom,num-grp-pins = <4>;
			qcom,pin-func = <0>;
			label = "spi2_sleep";
			spi_2_sleep: spi_2_sleep {
				drive-strength = <2>;
				bias-pull-down;
			};
		};
		tdmb_pwr_pinctrl {
			qcom,pins = <&gp 95>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "tdmb_pwr_pinctrl";
			tdmb_pwr_on:tdmb_pwr_on {
				drive-strength = <2>;
				bias-disable;
			};
			tdmb_pwr_off: tdmb_pwr_off {
				drive-strength = <2>;
				output-low;
				bias-pull-down;
			};
		};
		tdmb_irq_pinctrl {
			qcom,pins = <&gp 94>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "tdmb_irq_pinctrl";
			tdmb_irq_on: tdmb_irq_on {
				drive-strength = <2>;
				bias-disable;
			};
			tdmb_irq_off: tdmb_irq_off {
				drive-strength = <2>;
				bias-pull-down;
			};
		};
		tdmb_fm_sel_pinctrl {
			qcom,pins = <&gp 56>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "tdmb_fm_sel_pinctrl";
			tdmb_fm_sel_on: tdmb_fm_sel_on {
				drive-strength = <2>;
				bias-disable;
			};
			tdmb_fm_sel_off: tdmb_fm_sel_off {
				drive-strength = <2>;
				bias-pull-down;
			};
		};
	};
	spi_2: spi@78b7000 { /* BLSP1 QUP2 */
		compatible = "qcom,spi-qup-v2";
		cell-index = <3>;
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "spi_physical", "spi_bam_physical";
		reg = <0x78B7000 0x600>, <0x7884000 0x23000>;
		interrupt-names = "spi_irq", "spi_bam_irq";
		interrupts = <0 97 0>, <0 238 0>;
		spi-max-frequency = <11520000>;
		clocks = <&clock_gcc clk_gcc_blsp1_ahb_clk>,
			 <&clock_gcc clk_gcc_blsp1_qup3_spi_apps_clk>;
		clock-names = "iface_clk", "core_clk";

		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&spi_2_active>;
		pinctrl-1 = <&spi_2_sleep>;

		qcom,gpio-mosi = <&msm_gpio 8 0>;
		qcom,gpio-miso = <&msm_gpio 9 0>;
		qcom,gpio-cs0 = <&msm_gpio 10 0>;
		qcom,gpio-clk = <&msm_gpio 11 0>;

		qcom,use-bam;
		qcom,use-pinctrl;
		qcom,bam-consumer-pipe-index = <8>;
		qcom,bam-producer-pipe-index = <9>;
		qcom,infinite-mode = <0>;
		qcom,ver-reg-exists;
		qcom,master-id = <86>;

		tdmb-spi@0 {
			compatible = "tdmb_spi_comp";
			reg = <0>;
			spi-max-frequency = <11520000>;

		};
	};

	tdmb_data {
		compatible = "samsung,tdmb";
		pinctrl-names = "tdmb_on", "tdmb_off", "tdmb_gpio_init";
		pinctrl-0 = <&tdmb_pwr_on &tdmb_irq_on &tdmb_fm_sel_on>;
		pinctrl-1 = <&tdmb_pwr_off &tdmb_irq_off &tdmb_fm_sel_off>;
		pinctrl-2 = <&tdmb_pwr_off &tdmb_irq_off &spi_2_sleep &tdmb_fm_sel_off>;
		tdmb_pwr_en = <&msm_gpio 95 0>;
		tdmb_irq = <&msm_gpio 94 0>;
		tdmb_fm_ant_sel = <&msm_gpio 56 0>;
		tdmb_fm_ant_ldo = "8916_l18";
		tdmb_use_irq;
	};

};
